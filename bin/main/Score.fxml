<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text fx:id="txtNumber" fill="WHITE" stroke="BLACK" strokeType="OUTSIDE" text="1." wrappingWidth="15.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Text>
      <Region HBox.hgrow="ALWAYS" />
      <Text fx:id="txtName" fill="WHITE" layoutX="10.0" layoutY="23.0" stroke="BLACK" strokeType="OUTSIDE" text="WWWW" wrappingWidth="60.0" HBox.hgrow="ALWAYS">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Text>
      <Region layoutX="18.0" layoutY="10.0" HBox.hgrow="ALWAYS" />
      <Text fx:id="txtTime" fill="WHITE" layoutX="33.0" layoutY="23.0" stroke="BLACK" strokeType="OUTSIDE" text="1000:30" textAlignment="RIGHT" wrappingWidth="50.0" HBox.hgrow="ALWAYS">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Text>
   </children>
</HBox>
